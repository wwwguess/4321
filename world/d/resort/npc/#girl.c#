inherit NPC;

void create()
{
        set_name("小翠", ({ "xiao cui", "cui", "xiao" }) );
        set("gender", "女性" );
        set("age", 15);
        set("str", 16);
        set("cor", 24);
        set("cps", 11);
        set("per", 27);
        set("int", 27);

        set("attitude", "peaceful");

        set("max_force", 200);
        set("force", 200);
        set("force_factor", 2);

        set("long",     "小翠是铁雪山庄的丫环。\n");


